{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "docs/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/docs/(?<path>.*)", "dest": "/docs/$path" },
    { "src": "/(?<path>.*)", "dest": "/docs/$path" }
  ]
}
