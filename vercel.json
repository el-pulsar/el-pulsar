{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "docs/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "elpulsar.app"
        }
      ],
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "docs.elpulsar.app"
        }
      ],
      "dest": "/docs/$1"
    },
    {
      "src": "/docs/(.*)",
      "dest": "/docs/$1"
    }
  ]
}
